@model IEnumerable<Proyecto_Verificable.Models.FormDispossessor>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DispossessorRunRut)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DispossessorEntitlement)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DispossessorPercentNotCredited)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Form)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DispossessorRunRut)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DispossessorEntitlement)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DispossessorPercentNotCredited)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Form.FormId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FormDispossessorId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FormDispossessorId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FormDispossessorId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
